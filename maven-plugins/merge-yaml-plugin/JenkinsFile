def PomFile = 'maven-plugins/merge-yaml-plugin/pom.xml'
pipeline {
   agent any
   stages {
      stage('compile') {
           steps {
              // Run Maven on a Unix agent.
              sh "mvn ${env.STAGE_NAME} -f ${PomFile}"
           }
      }
      stage('package') {
           steps {
              // Run Maven on a Unix agent.
              sh "mvn ${env.STAGE_NAME} -f ${PomFile}"
           }
      }
      stage('install') {
           steps {
              // Run Maven on a Unix agent.
                sh "mvn ${env.STAGE_NAME} -f ${PomFile}"
           }
      }
   }
}
